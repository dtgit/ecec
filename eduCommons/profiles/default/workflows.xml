<?xml version="1.0"?>
<object name="portal_workflow" meta_type="Plone Workflow Tool">
 <property
    name="title">Contains workflow definitions for your portal</property>
 <object name="content_workflow" meta_type="Workflow"/>
 <bindings>

  <type type_id="Course">
   <bound-workflow workflow_id="content_workflow"/>
  </type>
  <type type_id="Division">
   <bound-workflow workflow_id="content_workflow"/>
  </type>
  <type type_id="File">
   <bound-workflow workflow_id="content_workflow"/>
  </type>
  <type type_id="FSSFile">
   <bound-workflow workflow_id="content_workflow"/>
  </type>
  <type type_id="Image">
   <bound-workflow workflow_id="content_workflow"/>
  </type>
  <type type_id="Document">
   <bound-workflow workflow_id="content_workflow"/>
  </type>
  <type type_id="Folder">
   <bound-workflow workflow_id="content_workflow"/>
  </type>
  <type type_id="Feedback">
   <bound-workflow workflow_id="content_workflow"/>
  </type>

 </bindings>
</object>
